<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop ="45dp"
    android:paddingBottom ="45dp"
    android:background="#353535"
    android:id="@+id/dialogLayout"
    >

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/archivo_black"
        android:text="Rate"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/rateBtn1"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/star_unfilled"
        app:layout_constraintEnd_toStartOf="@+id/rateBtn2"
        app:layout_constraintTop_toTopOf="@+id/rateBtn3" />

    <ImageButton
        android:id="@+id/rateBtn2"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/star_unfilled"
        app:layout_constraintEnd_toStartOf="@+id/rateBtn3"
        app:layout_constraintTop_toTopOf="@+id/rateBtn3" />

    <ImageButton
        android:id="@+id/rateBtn3"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/star_unfilled"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageButton
        android:id="@+id/rateBtn4"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/star_unfilled"
        app:layout_constraintStart_toEndOf="@+id/rateBtn3"
        app:layout_constraintTop_toTopOf="@+id/rateBtn3" />

    <ImageButton
        android:id="@+id/rateBtn5"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/star_unfilled"
        app:layout_constraintStart_toEndOf="@+id/rateBtn4"
        app:layout_constraintTop_toTopOf="@+id/rateBtn3" />
</androidx.constraintlayout.widget.ConstraintLayout>